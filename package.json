{
  "name": "nordnet",
  "version": "1.0.3",
  "description": "An improved Node.js wrapper around the Nordnet nExt API.",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production babel lib/ --out-dir dist/",
    "lint": "eslint lib",
    "docs:lint": "documentation lint lib/**/*",
    "docs:build": "documentation build lib/**/* -f md -o API.md",
    "docs": "npm run docs:lint && npm run docs:build",
    "test": "npm run start && NODE_ENV=test NODE_PATH=dist ava tests/**/*.spec.js",
    "prepublish": "npm run lint && npm test && npm start"
  },
  "author": "Filip Hallqvist <filiphallqvist@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "ursa": "^0.9.4"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "documentation": "^4.0.0-rc.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
